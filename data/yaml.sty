#
# style file that reproduces the old FORM format
#
<prologue>
#
# file generated by <program>
#
<command_loop><command_line_loop># <command_data><end><end>#
<diagram>
#--#[[ d<diagram_index>:
#
---
<diagram_index> : 
  factor : <minus><symmetry_number>
  sign   : '<sign>'
  loops  : <loops>
  props  : <propagator_loop>
    <propagator_index> :  
<back> {
<back> u : <dual-vertex_index>, 
<back> v : <vertex_index>,
<back> f : <field>, 
<back> t : [type],
<back> s : '<field_sign>',
<back> m : <momentum>}<end>
  legs:<in_loop>
    <in_index> :
<back> {
<back> u : -<in_index>, 
<back> v : <vertex_index>,
<back> f : <field>, 
<back> t : [type],
<back> m : <momentum>}<end><out_loop>
    <leg_index> :
<back> {
<back> u : -<leg_index>, 
<back> v : <vertex_index>,
<back> f : <field>, 
<back> t : [type],
<back> m : <dual-momentum>}<end>
  verts:<vertex_loop>
    <vertex_index> :
<back> { struct : <ray_loop>[type]<end>, 
<back> fields : [[<ray_loop><field>,<end><back>]], 
<back> props : [[<ray_loop><propagator_index>,<end><back>]]
<back>}<end>
#
#--#]] d<diagram_index>:
<epilogue>
#
# end
#
<exit>

